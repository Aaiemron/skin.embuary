<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="WidgetSpacer">
		<param name="visible">true</param>
		<param name="height">200</param>
		<definition>
			<control type="label" id="$PARAM[id]">
				<width>1</width>
				<height>$PARAM[height]</height>
				<texturefocus />
				<texturenofocus />
				<label/>
				<font/>
				<visible>$PARAM[visible]</visible>
			</control>
		</definition>
	</include>
	<include name="progress">
		<param name="visible">true</param>
		<param name="width">100</param>
		<param name="height">2</param>
		<param name="top">0</param>
		<param name="left">0</param>
		<definition>
			<control type="progress">
				<top>$PARAM[top]</top>
				<left>$PARAM[left]</left>
				<width>$PARAM[width]</width>
				<height>$PARAM[height]</height>
				<texturebg colordiffuse="black" border="2">common/ProgressBack.png</texturebg>
				<lefttexture>-</lefttexture>
				<midtexture colordiffuse="emby"  border="2">common/ProgressBar.png</midtexture>
				<righttexture>-</righttexture>
				<info>ListItem.Progress</info>
				<visible>$PARAM[visible]</visible>
			</control>
		</definition>
	</include>
</includes>
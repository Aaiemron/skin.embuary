<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">50</defaultcontrol>
	<backgroundcolor>background</backgroundcolor>
	<views>50,51,52,53</views>
	<menucontrol>9000</menucontrol>
	<controls>

		
		<include content="Background">
			<param name="id" value="$VAR[VisibleContainer]"/>
		</include>

		<control type="group">
			<animation effect="slide" start="0,0" end="0,-14" time="0" condition="Control.IsVisible(50)">Conditional</animation>
			<animation effect="slide" start="0,0" end="0,5" time="0" condition="Control.IsVisible(52)">Conditional</animation>
			<top>93</top>
			<width>1920</width>
			<height>40</height>
			<control type="group">
				<include content="JumpToLetter">
					<param name="visible" value="[Container.HasNext | Container.HasPrevious] + $EXP[ExpJumpToLetter]"/>
				</include>
			</control>
			<control type="label">
				<visible>![Container.HasNext | Container.HasPrevious] | !$EXP[ExpJumpToLetter]</visible>
				<centerleft>50%</centerleft>
				<width>1215</width>
				<height>40</height>
				<font>Med18</font>
				<align>center</align>
				<aligny>center</aligny>
				<textcolor>softgrey</textcolor>
				<shadowcolor>text_shadow</shadowcolor>
				<label>$INFO[Container.SortMethod,$LOCALIZE[581] ","]$INFO[Container.SortOrder, / $LOCALIZE[580] ","]</label>
			</control>
		</control>

		<!-- wide 50 -->
		<control type="group">
			<visible>Control.IsVisible(50)</visible>
			<animation condition="Integer.IsGreater(Container(50).NumItems,6) + Integer.IsLess(Container(50).NumItems,10)" effect="slide" start="0,0" end="225,0">Conditional</animation>
			<animation condition="Integer.IsGreater(Container(50).NumItems,3) + Integer.IsLess(Container(50).NumItems,7)" effect="slide" start="0,0" end="450,0">Conditional</animation>
			<animation condition="Integer.IsLess(Container(50).NumItems,4)" effect="slide" start="0,0" end="675,0">Conditional</animation>
			<!-- poster -->
			<control id="50" type="panel">
				<top>77</top>
				<width>1920</width>
				<height>1080</height>
				<onup condition="Control.IsVisible(7000)">SetFocus(7000)</onup>
				<onup condition="!Control.IsVisible(7000)">SetFocus(6666)</onup>
				<ondown>noop</ondown>
				<onleft>9999</onleft>
				<onright>noop</onright>
				<scrolltime>200</scrolltime>
				<movement>2</movement>
				<orientation>horizontal</orientation>
				<preloaditems>4</preloaditems>
				<defaultcontrol always="true">0</defaultcontrol>
				<viewtype label="535">panel</viewtype>
				<pagecontrol>17</pagecontrol>
				<visible>true</visible>
				<itemlayout width="450" height="305">
					<control type="group">
						<left>70</left>
						<top>64</top>
						<include content="LandscapeLayoutDB">
							<param name="mainlabel" value="$VAR[MainListLabel]" />
							<param name="sublabel" value="" />
							<param name="focused" value="false" />
							<param name="id" value="50" />
						</include>
					</control>
				</itemlayout>
				<focusedlayout width="450" height="305">
					<control type="group">							
						<left>70</left>
						<top>64</top>
						<include content="LandscapeLayoutDB">
							<param name="mainlabel" value="$VAR[MainListLabel]" />
							<param name="sublabel" value="" />
							<param name="focused" value="true" />
							<param name="id" value="50" />
						</include>
					</control>
				</focusedlayout>
			</control>
			<include content="list_mask">
				<param name="height" value="1022"/>
				<param name="width" value="75"/>
				<param name="top" value="58"/>
				<param name="id" value="50"/>
				<param name="visible_forced" value="true"/>
			</include>
		</control>

		<!-- Poster 51 -->
		<control type="group">
			<visible>Control.IsVisible(51)</visible>
			<animation condition="Integer.IsGreaterOrEqual(Container(51).NumItems,11) + Integer.IsLessOrEqual(Container(51).NumItems,12)" effect="slide" start="0,0" end="128,0">Conditional</animation>
			<animation condition="Integer.IsGreaterOrEqual(Container(51).NumItems,9) + Integer.IsLessOrEqual(Container(51).NumItems,10)" effect="slide" start="0,0" end="257,0">Conditional</animation>
			<animation condition="Integer.IsGreaterOrEqual(Container(51).NumItems,7) + Integer.IsLessOrEqual(Container(51).NumItems,8)" effect="slide" start="0,0" end="385,0">Conditional</animation>
			<animation condition="Integer.IsGreaterOrEqual(Container(51).NumItems,5) + Integer.IsLessOrEqual(Container(51).NumItems,6)" effect="slide" start="0,0" end="514,0">Conditional</animation>
			<animation condition="Integer.IsGreaterOrEqual(Container(51).NumItems,3) + Integer.IsLessOrEqual(Container(51).NumItems,4)" effect="slide" start="0,0" end="642,0">Conditional</animation>
			<animation condition="Integer.IsLessOrEqual(Container(51).NumItems,2)" effect="slide" start="0,0" end="771,0">Conditional</animation>
			<!-- poster -->
			<control id="51" type="panel">
				<top>105</top>
				<width>auto</width>
				<height>1080</height>
				<onup condition="Control.IsVisible(7000)">SetFocus(7000)</onup>
				<onup condition="!Control.IsVisible(7000)">SetFocus(6666)</onup>
				<ondown>noop</ondown>
				<onleft>9999</onleft>
				<onright>noop</onright>
				<scrolltime>200</scrolltime>
				<movement>2</movement>
				<orientation>horizontal</orientation>
				<preloaditems>4</preloaditems>
				<defaultcontrol always="true">0</defaultcontrol>
				<viewtype label="535">panel</viewtype>
				<pagecontrol>17</pagecontrol>
				<visible>true</visible>
				<itemlayout width="257" height="445">
					<control type="group">
						<left>70.5</left>
						<top>64</top>
						<include content="PosterLayoutDB">
							<param name="mainlabel" value="$VAR[MainListLabel]" />
							<param name="sublabel" value="$VAR[SubListLabel]" />
							<param name="focused" value="false" />
							<param name="id" value="51" />
						</include>
					</control>
				</itemlayout>
				<focusedlayout width="257" height="445">
					<control type="group">							
						<left>70.5</left>
						<top>64</top>
						<include content="PosterLayoutDB">
							<param name="mainlabel" value="$VAR[MainListLabel]" />
							<param name="sublabel" value="$VAR[SubListLabel]" />
							<param name="focused" value="true" />
							<param name="id" value="51" />
							<param name="visible_forced" value="true"/>
						</include>
					</control>
				</focusedlayout>
			</control>			
			<include content="list_mask">
				<param name="height" value="1022"/>
				<param name="width" value="75"/>
				<param name="top" value="58"/>
				<param name="id" value="51"/>
				<param name="visible_forced" value="true"/>
			</include>
		</control>

		<!-- square 52 -->
		<control type="group">
			<visible>Control.IsVisible(52)</visible>
			<animation condition="Integer.IsGreaterOrEqual(Container(52).NumItems,7) + Integer.IsLessOrEqual(Container(52).NumItems,8)" effect="slide" start="0,0" end="180,0">Conditional</animation>
			<animation condition="Integer.IsGreaterOrEqual(Container(52).NumItems,5) + Integer.IsLessOrEqual(Container(52).NumItems,6)" effect="slide" start="0,0" end="360,0">Conditional</animation>
			<animation condition="Integer.IsGreaterOrEqual(Container(52).NumItems,3) + Integer.IsLessOrEqual(Container(52).NumItems,4)" effect="slide" start="0,0" end="540,0">Conditional</animation>
			<animation condition="Integer.IsLessOrEqual(Container(52).NumItems,2)" effect="slide" start="0,0" end="720,0">Conditional</animation>
			<!-- poster -->
			<control id="52" type="panel">
				<top>115</top>
				<width>1920</width>
				<height>1080</height>
				<onup condition="Control.IsVisible(7000)">SetFocus(7000)</onup>
				<onup condition="!Control.IsVisible(7000)">SetFocus(6666)</onup>
				<ondown>noop</ondown>
				<onleft>9999</onleft>
				<onright>noop</onright>
				<scrolltime>200</scrolltime>
				<movement>2</movement>
				<orientation>horizontal</orientation>
				<preloaditems>4</preloaditems>
				<defaultcontrol always="true">0</defaultcontrol>
				<viewtype label="535">panel</viewtype>
				<pagecontrol>17</pagecontrol>
				<visible>true</visible>
				<itemlayout width="360" height="440">
					<control type="group">
						<left>70</left>
						<top>64</top>
						<include content="SquareLayoutDB">
							<param name="mainlabel" value="$VAR[MainListLabel]" />
							<param name="sublabel" value="$VAR[SubListLabel]" />
							<param name="focused" value="false" />
							<param name="id" value="52" />
						</include>
					</control>
				</itemlayout>
				<focusedlayout width="360" height="440">
					<control type="group">							
						<left>70</left>
						<top>64</top>
						<include content="SquareLayoutDB">
							<param name="mainlabel" value="$VAR[MainListLabel]" />
							<param name="sublabel" value="$VAR[SubListLabel]" />
							<param name="focused" value="true" />
							<param name="id" value="52" />
						</include>
					</control>
				</focusedlayout>
			</control>
			<include content="list_mask">
				<param name="height" value="1022"/>
				<param name="width" value="75"/>
				<param name="top" value="58"/>
				<param name="id" value="52"/>
				<param name="visible_forced" value="true"/>
			</include>
		</control>

		<!-- list 53 -->
		<control type="group">
			<visible>Control.IsVisible(53)</visible>
			<!-- Thumb image -->
			<control type="group">
				<visible>true</visible>
				<right>70</right>
				<top>168</top>
				<width>590</width>
				<include content="LandscapeLayoutBig"/>

				<control type="textbox">
					<top>430</top>
					<width>590</width>
					<height>500</height>
					<label>$INFO[ListItem.TagLine,,[CR][CR]][COLOR lightgrey]$INFO[ListItem.Genre][/COLOR]</label>
					<font>Reg22</font>
					<shadowcolor>text_shadow</shadowcolor>
					<align>center</align>
				</control>
			</control>
			<!-- List -->
			<control id="53" type="list">
				<top>168</top>
				<width>1230</width>
				<height>916</height>
				<onup>noop</onup>
				<ondown>noop</ondown>
				<onleft>9999</onleft>
				<onright>noop</onright>
				<defaultcontrol always="true">0</defaultcontrol>
				<focusposition>1</focusposition>
				<orientation>vertical</orientation>
				<viewtype label="535">list</viewtype>
				<pagecontrol>18</pagecontrol>
				<scrolltime>0</scrolltime>
				<preloaditems>4</preloaditems>
				<visible>true</visible>
				<itemlayout height="110" width="1160">
					<control type="group">
						<left>70</left>
						<include content="ListLayout">
						</include>
					</control>
				</itemlayout>
				<focusedlayout height="110" width="1160">
					<control type="group">
						<left>70</left>
						<include content="ListLayout">
							<param name="focused" value="true"/>
						</include>
					</control>
				</focusedlayout>
			</control>
			<include content="list_mask_vertical">
				<param name="height" value="65"/>
				<param name="id" value="53"/>
				<param name="visible_forced" value="true"/>
			</include>
		</control>

		<!-- header bar -->
		<include content="HeaderBar">
			<param name="id">51</param>
			<param name="enabled">false</param>
			<param name="sidebar_id">9999</param>
		</include>
		<control type="scrollbar" id="17">
			<left>0</left>
			<bottom>0</bottom>
			<width>1920</width>
			<height>5</height>
			<visible>true</visible>
			<texturesliderbackground colordiffuse="black">common/ProgressBack.png</texturesliderbackground>
			<texturesliderbar colordiffuse="emby">common/ProgressBar.png</texturesliderbar>
			<texturesliderbarfocus/>
			<textureslidernib/>
			<textureslidernibfocus/>
			<orientation>horizontal</orientation>
			<showonepage>false</showonepage>
			<visible>!Control.IsVisible(53)</visible>
		</control>
		<control type="scrollbar" id="18">
			<top>58</top>
			<left>0</left>
			<bottom>0</bottom>
			<width>5</width>
			<height>1042</height>
			<visible>true</visible>
			<texturesliderbackground colordiffuse="black">common/ProgressBack.png</texturesliderbackground>
			<texturesliderbar colordiffuse="emby">common/ProgressBar.png</texturesliderbar>
			<texturesliderbarfocus/>
			<textureslidernib/>
			<textureslidernibfocus/>
			<orientation>vertical</orientation>
			<showonepage>false</showonepage>
			<visible>Control.IsVisible(53)</visible>
		</control>
		<!-- diffuse -->
		<control type="image">
			<include>FullscreenDimensions</include>
			<texture colordiffuse="88000000">common/white.jpg</texture>
			<aspectratio>stretch</aspectratio>
			<visible>!Control.HasFocus(50) + !Control.HasFocus(51) + !Control.HasFocus(52) + !Control.HasFocus(53) + !ControlGroup(6666).HasFocus + !Control.HasFocus(7000)</visible>
			<include>Visible_Fade</include>
		</control>
		<!-- side panel -->
		<include content="SidePanel">
			<param name="onright">50</param>
		</include>
	</controls>
</window>

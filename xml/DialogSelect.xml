<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">3</defaultcontrol>
	<include>Dialog_PopupOpenClose</include>
	<controls>
		<include content="DiffuseMask">
			<param name="visible">true</param>
		</include>
		<control type="group">
			<centerleft>50%</centerleft>
			<centertop>50%</centertop>
			<width>373</width>
			<height>531</height>
			<control type="image">
				<width>373</width>
				<height>531</height>
				<texture colordiffuse="panel" border="10">common/dialog_back.png</texture>
			</control>
			<control type="list" id="3">
				<centerleft>50%</centerleft>
				<top>10</top>
				<width>363</width>
				<height>450</height>
				<pagecontrol>17</pagecontrol>
				<onup>noop</onup>
				<onleft>close</onleft>
				<onright>close</onright>
				<ondown>7</ondown>
				<itemlayout width="363" height="50">
					<control type="image">
						<width>100%</width>
						<height>50</height>
						<texture colordiffuse="panel">common/white.jpg</texture>
					</control>
					<control type="label">
						<width>100%</width>
						<height>50</height>
						<aligny>center</aligny>
						<textoffsetx>20</textoffsetx>
						<font>med18</font>
						<textcolor>white</textcolor>
						<textshadow>text_shadow</textshadow>
						<label>$INFO[ListItem.Label]</label>
					</control>
				</itemlayout>
				<focusedlayout width="363" height="50">
					<control type="image">
						<width>100%</width>
						<height>50</height>
						<texture colordiffuse="panel">common/white.jpg</texture>
						<visible>!Control.HasFocus(3)</visible>
					</control>
					<control type="image">
						<width>100%</width>
						<height>50</height>
						<texture colordiffuse="panel_hover">common/white.jpg</texture>
						<visible>Control.HasFocus(3)</visible>
					</control>
					<control type="label">
						<width>100%</width>
						<height>50</height>
						<aligny>center</aligny>
						<textoffsetx>20</textoffsetx>
						<font>med18</font>
						<textcolor>white</textcolor>
						<textshadow>text_shadow</textshadow>
						<label>$INFO[ListItem.Label]</label>
					</control>
				</focusedlayout>
			</control>
			<control type="list" id="6">
				<centerleft>50%</centerleft>
				<top>10</top>
				<width>363</width>
				<height>450</height>
				<pagecontrol>17</pagecontrol>
				<onup>noop</onup>
				<onleft>close</onleft>
				<onright>close</onright>
				<ondown>7</ondown>
				<itemlayout width="363" height="50">
					<control type="image">
						<width>100%</width>
						<height>50</height>
						<texture colordiffuse="panel">common/white.jpg</texture>
					</control>
					<control type="label">
						<width>100%</width>
						<height>50</height>
						<aligny>center</aligny>
						<textoffsetx>20</textoffsetx>
						<font>med18</font>
						<textcolor>white</textcolor>
						<textshadow>text_shadow</textshadow>
						<label>$INFO[ListItem.Label]</label>
					</control>
				</itemlayout>
				<focusedlayout width="363" height="50">
					<control type="image">
						<width>100%</width>
						<height>50</height>
						<texture colordiffuse="panel">common/white.jpg</texture>
						<visible>!Control.HasFocus(3)</visible>
					</control>
					<control type="image">
						<width>100%</width>
						<height>50</height>
						<texture colordiffuse="panel_hover">common/white.jpg</texture>
						<visible>Control.HasFocus(3)</visible>
					</control>
					<control type="label">
						<width>100%</width>
						<height>50</height>
						<aligny>center</aligny>
						<textoffsetx>20</textoffsetx>
						<font>med18</font>
						<textcolor>white</textcolor>
						<textshadow>text_shadow</textshadow>
						<label>$INFO[ListItem.Label]</label>
					</control>
				</focusedlayout>
			</control>
			<!-- Scrollbar -->
			<control id="17" type="scrollbar">
				<right>5</right>
				<top>5</top>
				<width>5</width>
				<height>460</height>
				<onup>noop</onup>
				<ondown>noop</ondown>
				<onleft>Close</onleft>
				<onright>Close</onright>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
				<texturesliderbackground colordiffuse="black">common/ProgressBack.png</texturesliderbackground>
				<texturesliderbar colordiffuse="emby">common/ProgressBar.png</texturesliderbar>
				<texturesliderbarfocus colordiffuse="emby">common/ProgressBar.png</texturesliderbarfocus>
			</control>
			<control type="image">
				<centerleft>50%</centerleft>
				<bottom>65</bottom>
				<width>363</width>
				<height>1</height>
				<texture colordiffuse="panel_hover">common/white.jpg</texture>
			</control>
			<control type="button" id="7">
				<centerleft>50%</centerleft>
				<bottom>10</bottom>
				<width>363</width>
				<height>50</height>
				<align>center</align>
				<aligny>center</aligny>
				<textoffsetx>20</textoffsetx>
				<font>med18</font>
				<textcolor>white</textcolor>
				<textshadow>text_shadow</textshadow>
				<texturefocus colordiffuse="panel_hover">common/white.jpg</texturefocus>
				<texturenofocus colordiffuse="panel">common/white.jpg</texturenofocus>
				<onup>3</onup>
			</control>
		</control>
	</controls>
</window>

<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>3</defaultcontrol>
	<backgroundcolor>background</backgroundcolor>
	<controls>
		<control type="label" id="2">
			<include>HiddenObject</include>
		</control>
		<include>Background</include>
		<control type="group">
			<include>Window_OpenClose</include>
			<!-- left -->
			<include content="ContentPanel">
			</include>
			<control type="group">
				<top>73</top>
				<include content="SideMenuHeadline">
					<param name="nr">1</param>
					<param name="label">$LOCALIZE[5]</param>
				</include>
				<control type="grouplist" id="3">
					<description>button area</description>
					<left>0</left>
					<top>55</top>
					<bottom>130</bottom>
					<width>470</width>
					<usecontrolcoords>true</usecontrolcoords>
					<onleft>9999</onleft>
					<onright>5</onright>
					<onup>6666</onup>
					<ondown>20</ondown>
				</control>
				<control type="button" id="10">
					<description>Default Category Button</description>
					<width>400</width>
					<height>50</height>
					<align>left</align>
					<aligny>center</aligny>
					<font>Reg22</font>
					<textoffsetx>50</textoffsetx>
					<textcolor>panel_text</textcolor>
					<focusedcolor>panel_text</focusedcolor>
					<selectedcolor>panel_text</selectedcolor>
					<shadowcolor>text_shadow</shadowcolor>
					<texturefocus colordiffuse="panel_hover">common/white.jpg</texturefocus>
					<include>NoTextures</include>
				</control>
			</control>
			<control type="group">
				<bottom>50</bottom>
				<height>105</height>
				<control type="group">
					<include content="SideMenuHeadline">
						<param name="nr">1</param>
						<param name="label">$LOCALIZE[31036]</param>
					</include>
				</control>
				<control type="radiobutton" id="20">
					<top>55</top>
					<width>400</width>
					<height>50</height>
					<align>left</align>
					<aligny>center</aligny>
					<font>Reg22</font>
					<textoffsetx>50</textoffsetx>
					<textcolor>panel_text</textcolor>
					<focusedcolor>panel_text</focusedcolor>
					<selectedcolor>panel_text</selectedcolor>
					<shadowcolor>text_shadow</shadowcolor>
					<texturefocus colordiffuse="panel_hover">common/white.jpg</texturefocus>
					<include>NoTextures</include>
					<onup>3</onup>
					<onleft>9999</onleft>
				</control>
			</control>
			<!-- right -->
			<control type="group">
				<control type="grouplist" id="5">
					<description>control area</description>
					<top>128</top>
					<left>400</left>
					<pagecontrol>60</pagecontrol>
					<right>0</right>
					<bottom>50</bottom>
					<onleft>3</onleft>
					<onright>noop</onright>
					<onup>6666</onup>
					<ondown>noop</ondown>
					<usecontrolcoords>true</usecontrolcoords>
				</control>
				<control type="button" id="7">
					<top>0</top>
					<description>Default Button</description>
					<include content="SettingsButton">
						<param name="textoffsetx">35</param>
					</include>
				</control>
				<control type="radiobutton" id="8">
					<description>Default RadioButton</description>
					<include content="SettingsButton">
						<param name="textoffsetx">35</param>
					</include>
				</control>
				<control type="spincontrolex" id="9">
					<description>Default SpinControlex</description>
					<include content="SettingsButton">
						<param name="textoffsetx">35</param>
					</include>
				</control>
				<control type="sliderex" id="13">
					<description>Default Slider</description>
					<include content="SettingsButton">
						<param name="textoffsetx">35</param>
					</include>
				</control>
				<control type="image" id="11">
					<description>separator image</description>
					<visible>false</visible>
				</control>
				<control type="label" id="14">
					<description>Default Label</description>
					<height>50</height>
					<font>med22</font>
					<textoffsetx>25</textoffsetx>
					<textcolor>panel_text</textcolor>
					<shadowcolor>text_shadow</shadowcolor>
					<aligny>center</aligny>
				</control>				
				<include content="list_mask_vertical">
					<param name="height" value="70"/>
					<param name="left" value="400"/>
					<param name="right" value="0"/>
					<param name="id" value="5"/>
					<param name="visible_forced" value="true"/>
				</include>
			</control>
			<control type="scrollbar" id="60">
				<right>0</right>
				<top>58</top>
				<width>5</width>
				<bottom>0</bottom>
				<orientation>vertical</orientation>
			</control>
			<control type="group">
				<include>OpenClose_Right</include>
				<control type="textbox" id="6">
					<description>description area</description>
					<left>510</left>
					<bottom>25</bottom>
					<right>60</right>
					<height>102</height>
					<font>font12</font>
					<align>justify</align>
					<textcolor>button_focus</textcolor>
					<autoscroll time="3000" delay="5000" repeat="5000">true</autoscroll>
				</control>
			</control>
		</control>
		<!-- header bar -->
		<include content="HeaderBar">		
			<param name="id">3</param>
			<param name="enabled">false</param>
			<param name="sidebar_id">9999</param>
			<param name="custom_animation">[[Control.IsVisible(54) + Container(540).HasPrevious]] | [Control.IsVisible(55) + Container(550).HasPrevious]</param>
		</include>
		<!-- side panel -->
		<include content="SidePanel">
			<param name="onright">3</param>
		</include>
	</controls>
</window>

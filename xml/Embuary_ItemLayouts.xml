<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<!-- Common Layout Includes -->
	<include name="widget_headline">
		<param name="top">25</param>
		<param name="left">67</param>
		<definition>
			<control type="label">
				<top>$PARAM[top]</top>
				<left>$PARAM[left]</left>
				<width>1000</width>
				<height>87</height>
				<font>Med20</font>
				<align>left</align>
				<aligny>center</aligny>
				<textcolor>white</textcolor>
				<label>$PARAM[headline]</label>
			</control>
		</definition>
	</include>

	<include name="widget_labels">
		<param name="visible_labels">true</param>
		<param name="mainlabel">$VAR[MainListLabel]</param>
		<param name="sublabel">$VAR[SubListLabel]</param>
		<param name="textoffsetx" default="0">$PARAM[textoffsetx]</param>
		<definition>
			<control type="group">
				<top>$PARAM[top]</top>
				<width>$PARAM[width]</width>
				<visible>$PARAM[visible_labels]</visible>
				<control type="label">
					<width>$PARAM[width]</width>
					<label>$PARAM[mainlabel]</label>
					<font>Reg22</font>
					<shadowcolor>text_shadow</shadowcolor>
					<scroll>$PARAM[focused] + Control.HasFocus($PARAM[id])</scroll>
					<align>center</align>
					<textoffsetx>$PARAM[textoffsetx]</textoffsetx>
				</control>
				<control type="label">
					<top>30</top>
					<width>$PARAM[width]</width>
					<label>$PARAM[sublabel]</label>
					<font>Reg22</font>
					<textcolor>lightgrey</textcolor>
					<shadowcolor>text_shadow</shadowcolor>
					<scroll>$PARAM[focused] + Control.HasFocus($PARAM[id])</scroll>
					<align>center</align>
					<textoffsetx>$PARAM[textoffsetx]</textoffsetx>
				</control>
			</control>
		</definition>
	</include>

	<include name="bubble_overlay">
		<control type="image" id="2">
			<right>10</right>
			<top>10</top>
			<width>32</width>
			<height>28</height>
			<texture colordiffuse="red">$VAR[PVRTimerIcon]</texture>
			<aspectratio align="right">keep</aspectratio>
		</control>
		<control type="group">
			<visible>[String.IsEqual(ListItem.DBtype,tvshow) + !String.IsEmpty(ListItem.Property(UnWatchedEpisodes)) + Integer.IsLess(ListItem.Property(UnWatchedEpisodes),1)] | String.IsEqual(ListItem.Overlay,OverlayWatched.png) | Integer.IsGreater(ListItem.Property(UnWatchedEpisodes),0)</visible>
			<right>8</right>
			<top>9</top>
			<width>44</width>
			<height>44</height>
			<control type="image">
				<width>44</width>
				<height>44</height>
				<texture colordiffuse="emby">items/bubble_small.png</texture>
				<aspectratio>keep</aspectratio>
			</control>
			<control type="label">
				<top>7.5</top>
				<left>1</left>
				<width>44</width>
				<height>44</height>
				<label>$INFO[ListItem.Property(UnWatchedEpisodes)]</label>
				<font>Bold14</font>
				<shadowcolor>text_shadow</shadowcolor>
				<align>center</align>
				<visible>Integer.IsGreater(ListItem.Property(UnWatchedEpisodes),0)</visible>
			</control>
			<control type="label">
				<top>9</top>
				<left>2</left>
				<width>44</width>
				<height>44</height>
				<label></label>
				<font>FontAwesome20Light</font>
				<shadowcolor>text_shadow</shadowcolor>
				<align>center</align>
				<visible>String.IsEqual(ListItem.Overlay,OverlayWatched.png) | [String.IsEqual(ListItem.DBtype,tvshow) + !String.IsEmpty(ListItem.Property(UnWatchedEpisodes)) + Integer.IsLess(ListItem.Property(UnWatchedEpisodes),1)]</visible>
			</control>
		</control>
	</include>
	<include name="bubble_mini_overlay">
		<control type="group">
			<visible>String.IsEqual(ListItem.Overlay,OverlayWatched.png)</visible>
			<left>$PARAM[left]</left>
			<top>$PARAM[top]</top>
			<width>36</width>
			<height>36</height>
			<control type="image">
				<width>36</width>
				<height>36</height>
				<texture colordiffuse="emby">items/bubble_mini.png</texture>
				<aspectratio>keep</aspectratio>
			</control>
			<control type="label">
				<top>8</top>
				<left>2</left>
				<width>36</width>
				<height>36</height>
				<label></label>
				<font>FontAwesome16Light</font>
				<shadowcolor>text_shadow</shadowcolor>
				<align>center</align>
				<visible>String.IsEqual(ListItem.Overlay,OverlayWatched.png)</visible>
			</control>
		</control>
	</include>

	<include name="bubble_play">
		<control type="group">
			<visible>[!String.IsEmpty(ListItem.DBtype) | String.StartsWith(ListItem.Path,pvr://)]</visible>
			<right>10</right>
			<bottom>10</bottom>
			<width>38</width>
			<height>38</height>
			<control type="image">
				<width>38</width>
				<height>38</height>
				<texture colordiffuse="a9000000">items/bubble_mask.png</texture>
				<aspectratio>keep</aspectratio>
			</control>
			<control type="label">
				<top>11</top>
				<left>1</left>
				<width>38</width>
				<height>38</height>
				<label></label>
				<font>FontAwesome14Light</font>
				<shadowcolor>text_shadow</shadowcolor>
				<align>center</align>
			</control>
		</control>
	</include>

	<include name="list_mask">
		<param name="top" default="94"/>
		<param name="width" default="100"/>
		<param name="visible_forced" default="false"/>
		<definition>
			<control type="group">
				<visible>[Container($PARAM[id]).HasNext + Control.HasFocus($PARAM[id])] | [Container($PARAM[id]).HasNext + $PARAM[visible_forced]]</visible>1
				<animation effect="fade" start="0" end="100" time="200" tween="sine">Visible</animation>
				<animation effect="fade" start="100" end="0" time="0">Hidden</animation>
				<control type="image">
					<top>$PARAM[top]</top>
					<right>0</right>
					<width>$PARAM[width]</width>
					<height>$PARAM[height]</height>
					<texture>items/listmask.png</texture>
					<aspectratio>stretch</aspectratio>
				</control>
				<control type="label">
					<top>$PARAM[top]</top>
					<right>0</right>
					<width>55</width>
					<height>$PARAM[height]</height>
					<label></label>
					<font>Material50</font>
					<shadowcolor>text_shadow</shadowcolor>
					<align>center</align>					
					<aligny>center</aligny>
				</control>
			</control>
		</definition>
	</include>

	<include name="list_mask_vertical">
		<param name="height" default="145"/>
		<param name="width" default="1920"/>
		<param name="visible_forced" default="false"/>
		<definition>
			<control type="group">
				<visible>[Container($PARAM[id]).HasNext + Control.HasFocus($PARAM[id])] | [Container($PARAM[id]).HasNext + $PARAM[visible_forced]]</visible>1
				<animation effect="fade" start="0" end="100" time="200" tween="sine">Visible</animation>
				<animation effect="fade" start="100" end="0" time="0">Hidden</animation>
				<control type="image">
					<bottom>0</bottom>
					<right>0</right>
					<width>$PARAM[width]</width>
					<height>$PARAM[height]</height>
					<texture>items/listmask_vertical.png</texture>
					<aspectratio>stretch</aspectratio>
				</control>
				<control type="label">
					<bottom>0</bottom>
					<width>1920</width>
					<height>35</height>
					<label></label>
					<font>Material50</font>
					<shadowcolor>text_shadow</shadowcolor>
					<align>center</align>					
					<aligny>center</aligny>
				</control>
			</control>
		</definition>
	</include>


	<include name="WidgetSpacer">
		<param name="visible">true</param>
		<param name="height">200</param>
		<definition>
			<control type="label" id="$PARAM[id]">
				<width>1</width>
				<height>$PARAM[height]</height>
				<texturefocus />
				<texturenofocus />
				<label/>
				<font/>
				<visible>$PARAM[visible]</visible>
			</control>
		</definition>
	</include>

	<include name="progress">
		<param name="visible">true</param>
		<param name="width">100</param>
		<param name="height">2</param>
		<param name="top">0</param>
		<param name="left">0</param>
		<definition>
			<control type="progress">
				<top>$PARAM[top]</top>
				<left>$PARAM[left]</left>
				<width>$PARAM[width]</width>
				<height>$PARAM[height]</height>
				<texturebg colordiffuse="black" border="2">common/ProgressBack.png</texturebg>
				<lefttexture>-</lefttexture>
				<midtexture colordiffuse="emby"  border="2">common/ProgressBar.png</midtexture>
				<righttexture>-</righttexture>
				<info>ListItem.Progress</info>
				<visible>String.StartsWith(ListItem.Path,pvr://channels)</visible>
			</control>
			<control type="progress">
				<top>$PARAM[top]</top>
				<left>$PARAM[left]</left>
				<width>$PARAM[width]</width>
				<height>$PARAM[height]</height>
				<texturebg colordiffuse="black" border="2">common/ProgressBack.png</texturebg>
				<lefttexture>-</lefttexture>
				<midtexture colordiffuse="emby"  border="2">common/ProgressBar.png</midtexture>
				<righttexture>-</righttexture>
				<info>ListItem.PercentPlayed</info>
				<visible>!String.StartsWith(ListItem.Path,pvr://channels) + Integer.IsGreater(ListItem.PercentPlayed,0)</visible>
			</control>
		</definition>
	</include>


	<!-- Poster -->
	<include name="PosterLayout">
		<param name="mainlabel">$VAR[MainListLabel]</param>

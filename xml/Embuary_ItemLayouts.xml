		<param name="thumblabel">$VAR[MainListLabel]</param>
		<param name="focused">false</param>
		<param name="visible">true</param>
		<definition>
			<control type="group">
				<visible>$PARAM[visible]</visible>
				<control type="image">
					<top>-2</top>
					<left>-2</left>
					<width>282</width>
					<height>420</height>
					<texture>items/shadow_poster.png</texture>
				</control>
				<control type="image">
					<width>276</width>
					<height>414</height>
					<texture colordiffuse="green" scalediffuse="false" diffuse="items/mask_poster.png">common/white.jpg</texture>
					<aspectratio>stretch</aspectratio>
					<visible>String.IsEmpty(ListItem.Art(poster))</visible>
				</control>
				<control type="image">
					<width>276</width>
					<height>414</height>
					<texture colordiffuse="thumb_background" scalediffuse="false" diffuse="items/mask_poster.png">common/white.jpg</texture>
					<aspectratio>stretch</aspectratio>
					<visible>!String.IsEmpty(ListItem.Art(poster))</visible>
				</control>
				<control type="textbox">
					<top>10</top>
					<left>10</left>
					<width>246</width>
					<height>394</height>
					<font>Light24</font>
					<align>center</align>
					<aligny>center</aligny>
					<label>$PARAM[thumblabel]</label>
					<visible>String.IsEmpty(ListItem.Art(poster))</visible>
				</control>
				<control type="image">
					<width>276</width>
					<height>414</height>
					<texture scalediffuse="false" diffuse="items/mask_poster.png" background="true">$INFO[ListItem.Art(poster)]</texture>
					<aspectratio>scale</aspectratio>
				</control>
				<control type="group">
					<width>276</width>
					<height>414</height>
					<include>bubble_overlay</include>
					<include content="bubble_play">
						<param name="top" value="367"/>
					</include>
				</control>
				<include content="progress">
					<param name="top" value="409"/>
					<param name="width" value="276"/>
				</include>
				<control type="image">
					<left>-12</left>
					<top>-7</top>
					<width>300</width>
					<height>437</height>
					<texture colordiffuse="focused">items/focus_poster.png</texture>
					<aspectratio>scale</aspectratio>
					<visible>$PARAM[focused] + Control.HasFocus($PARAM[id])</visible>
					<include condition="$PARAM[focused]">Animation_FocusTextureFade</include>
				</control>
				<include content="widget_labels">
					<param name="top">426</param>
					<param name="width">276</param>
					<param name="id">$PARAM[id]</param>
					<param name="mainlabel">$PARAM[mainlabel]</param>
					<param name="sublabel">$PARAM[sublabel]</param>
					<param name="focused">$PARAM[focused]</param>
					<param name="visible_labels">$PARAM[visible_labels]</param>
				</include>
			</control>
		</definition>
	</include>

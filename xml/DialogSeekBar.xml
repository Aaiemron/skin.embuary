<?xml version="1.0" encoding="UTF-8"?>
<window>
	<visible>Player.Seeking | Player.DisplayAfterSeek | [Player.Paused + !Player.Caching] | Player.Forwarding | Player.Rewinding | Player.ShowInfo | Window.IsActive(fullscreeninfo) | Window.IsActive(videoosd) | Window.IsActive(musicosd) | Window.IsActive(playerprocessinfo) | !IsEmpty(Player.SeekNumeric)</visible>
	<visible>!Window.IsActive(sliderdialog)</visible>
	<visible>Window.IsActive(visualisation) | Window.IsActive(fullscreenvideo)</visible>
	<zorder>0</zorder>
	<controls>
		<control type="group">
			<!-- Seekbar nib -->
			<control type="slider" id="401">
				<visible>Player.Forwarding | Player.Rewinding | Player.Seeking | !String.IsEmpty(Player.SeekOffset)</visible>
				<bottom>103</bottom>
				<left>309</left>
				<right>31</right>
				<height>8</height>
				<orientation>horizontal</orientation>
				<texturesliderbar colordiffuse="00ffffff">common/ProgressBack.png</texturesliderbar>
				<textureslidernib colordiffuse="emby">views/player/nib_bubble.png</textureslidernib>
				<textureslidernibfocus colordiffuse="emby">views/player/nib_bubble.png</textureslidernibfocus>
				<info>Player.SeekTime</info>
			</control>
			<control type="group">
				<visible>Player.Forwarding | Player.Rewinding | Player.Seeking | !String.IsEmpty(Player.SeekOffset)</visible>
				<include>OSD_Seek_Position</include>
				<left>309</left>
				<bottom>125</bottom>
				<width>158</width>
				<height>70</height>
				<control type="image">
					<width>158</width>
					<height>70</height>
					<texture colordiffuse="panel">common/white.jpg</texture>
					<bordertexture colordiffuse="grey" border="1">common/white.jpg</bordertexture>
					<bordersize>1</bordersize>
				</control>
				<control type="label">
					<width>158</width>
					<height>70</height>
					<align>center</align>
					<aligny>center</aligny>
					<font>med26</font>
					<textcolor>ffffffff</textcolor>
					<label>$INFO[Player.SeekTime]</label>
				</control>
			</control>
			<!-- Forwarding Rewinding Indicator -->
			<control type="group">
				<!--<visible>!Player.Paused + [Player.Forwarding | Player.Rewinding]</visible>-->
				<control id="1" type="progress">
					<left>92</left>
					<bottom>200</bottom>
					<width>1580</width>
					<!--<width>1735</width>-->
					<height>11</height>
					<reveal>true</reveal>
					<info>Player.Progress</info>
					<texturebg>-</texturebg>
					<lefttexture>-</lefttexture>
					<midtexture>-</midtexture>
					<righttexture>-</righttexture>
					<overlaytexture>-</overlaytexture>
					<textureslidernib>osd/osd_seek_bar.png</textureslidernib>
					<textureslidernibfocus>osd/osd_seek_bar.png</textureslidernibfocus>
				</control>



			</control>
		</control>
	</controls>
</window>
